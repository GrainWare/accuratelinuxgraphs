// generate random data
const randomData = (n: number) => {
	const data = [];
	for (let i = 0; i < n; i++) {
		data.push({
			something: Math.random() * 100,
			somethingElse: Math.random() * 100,
		});
	}
	return data;
};
const data = randomData(100);
